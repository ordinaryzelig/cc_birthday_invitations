<ul>
  <% song_requests.each do |song_request| %>
    <li>
      <strong><%= song_request.title %></strong>
      by
      <%= song_request.artist %>
      <% if editable %>
        <%= link_to 'Delete', invitation_song_request_url(song_request.invitation, song_request), :method => :delete %>
      <% end %>
    </li>
  <% end %>
</ul>
